<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fw_contact_index" path="/contact.html" methods="GET">
        <default key="_controller">FrequenceWebContactBundle:Default:index</default>
    </route>

    <route id="fw_contact_submit" path="/contact.html" methods="POST">
        <default key="_controller">FrequenceWebContactBundle:Default:submit</default>
    </route>
</routes>
